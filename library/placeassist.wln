    "type" -> "input", "data" -> ".exports\nPlaceAssist[init_:{0,0}] := \
With[{uid = CreateUUID[]},\n\tLeakyModule[{variable = init, text = init // \
ToString, gizmo},\n         gizmo[var_, text_] := With[{v = var // \
Evaluate},\n        \t{\n        \t\tPointSize[0.05], Red, \n        \
\t\tEventHandler[\n        \t\t\tPoint[v], \n        \t\t\t{\n        \
\t\t\t\t\"drag\"->Function[xy, var = xy; text = xy // ToString]\n        \
\t\t\t}\n        \t\t]\n        \t}\n        ];\n        \n        \
SetAttributes[gizmo, HoldAll];\n        \n\t\tWith[{fe = \
CreateFrontEndObject[TextView[text // Offload], uid]},\n\n    \t\t(* private \
*)\n    \t\tCreateFrontEndObject[With[{g = gizmo[variable, text]},\n    \
\t\t\tOffload[Static[g]; variable]\n    \t\t], uid, \
\"Type\"->\"Private\"];\n\n            (* public *)\n            fe\n        \
]\n\t]\n]", "display" -> "codemirror", "sign" -> "glamorize-b1e60", 
    "props" -> <|"hidden" -> False|>|>, 
