Component[OptionsPattern[]] := With[{
    Data = OptionValue["Data"]["Data"]
},

    With[{
        Results = Table[Database[Item, "Template"], {Item, Data}]
    },
        <ul class="max-h-96 overflow-y-auto pt-1 text-sm text-gray-700">
            <Results/>
        </ul>    
    ]
]

Options[Component] = {"Data" -> {}, "Event" -> ""}

Component